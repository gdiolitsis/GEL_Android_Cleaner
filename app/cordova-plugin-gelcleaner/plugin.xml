<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-gelcleaner"
        version="1.0.0"
        xmlns="http://apache.org/cordova/ns/plugins"
        xmlns:android="http://schemas.android.com/apk/res/android">

    <name>GELCleaner</name>
    <description>GEL Android Cleaner Native Plugin</description>
    <license>MIT</license>
    <keywords>gel, cleaner, android, cache</keywords>
    <js-module src="www/GelCleaner.js" name="GELCleaner">
        <clobbers target="GELCleaner" />
    </js-module>

    <platform name="android">
        <source-file src="src/android/GelCleaner.java" target-dir="src/com/gdiolitsis/gelcleaner" />
    </platform>
</plugin>
