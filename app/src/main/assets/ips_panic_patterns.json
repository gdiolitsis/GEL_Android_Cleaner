{
  "meta": {
    "name": "GEL iOS Panic Signature Pack",
    "version": "1.0",
    "scope": "offline",
    "engine": "rule-based",
    "author": "GDiolitsis Engine Lab (GEL)"
  },
  "source": "GEL Service Knowledge Base",
  "patterns": [

    {
      "id": "IPS-THERMAL-001",
      "domain": "Thermal / Sensors",
      "severity": "HIGH",
      "confidence": "HIGH",
      "cause": "Thermal sensor not responding (I2C line)",
      "recommendation": "Logic board inspection or flex replacement",
      "match": ["Missing sensor", "TG0", "TG1", "thermal", "sensor"]
    },

    {
      "id": "IPS-POWER-002",
      "domain": "Power / PMIC",
      "severity": "HIGH",
      "confidence": "MEDIUM",
      "cause": "Power management instability",
      "recommendation": "Battery + PMIC line check",
      "match": ["PMIC", "powerd", "shutdown", "shutdown cause"]
    },

    {
      "id": "IPS-WATCHDOG-003",
      "domain": "System / Watchdog",
      "severity": "MEDIUM",
      "confidence": "MEDIUM",
      "cause": "System freeze or watchdog deadlock",
      "recommendation": "iOS restore or update recommended",
      "match": ["watchdog", "userspace watchdog timeout", "timeout"]
    },

    {
      "id": "IPS-NAND-004",
      "domain": "Storage / NAND",
      "severity": "CRITICAL",
      "confidence": "HIGH",
      "cause": "Storage read/write failure",
      "recommendation": "Logic board storage repair required",
      "match": ["nand", "I/O error", "filesystem corruption", "storage"]
    },

    {
      "id": "IPS-BASEBAND-005",
      "domain": "Baseband / Modem",
      "severity": "HIGH",
      "confidence": "HIGH",
      "cause": "Baseband processor crash or communication failure",
      "recommendation": "Inspect baseband IC, RF section and power lines",
      "match": ["baseband", "panic", "modem"]
    },

    {
      "id": "IPS-MEMORY-006",
      "domain": "Memory / RAM",
      "severity": "CRITICAL",
      "confidence": "HIGH",
      "cause": "Unrecoverable memory fault detected",
      "recommendation": "Inspect RAM IC; board-level repair may be required",
      "match": ["memory", "panic", "ram"]
    },

    {
      "id": "IPS-KERNEL-007",
      "domain": "Kernel / OS",
      "severity": "MEDIUM",
      "confidence": "LOW",
      "cause": "Kernel assertion failure",
      "recommendation": "Evaluate software integrity; hardware correlation possible",
      "match": ["assert", "kernel"]
    }

  ]
}
