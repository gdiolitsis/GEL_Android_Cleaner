<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Pressed -->
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#D19A36"/>
            <stroke android:width="2dp" android:color="#FFB13F"/>
            <corners android:radius="14dp"/>
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp"/>
        </shape>
    </item>

    <!-- Default -->
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FFB13F"/>
            <stroke android:width="2dp" android:color="#FFC766"/>
            <corners android:radius="14dp"/>
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp"/>
        </shape>
    </item>

</selector>
